{
  "name": "countfast",
  "version": "2.0.0",
  "description": "A quiz-type vanilla JS app that helps to practice mental calculations.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --watch",
    "server": "nodemon server.js",
    "build": "cd public && browserify scripts/index.js -o bundle.js",
    "dev": "set NODE_ENV=development && npm run build && npm run server",
    "locally": "npm i && npm run dev",
    "publish": "git push -u origin master && git push heroku master && heroku open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaciejReimann/CountFast.git"
  },
  "author": "Maciej Reimann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaciejReimann/CountFast/issues"
  },
  "homepage": "https://github.com/MaciejReimann/CountFast#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "nodemon": "^1.18.6"
  }
}
